{
    "family": "webglimpse",
    "containerDefinitions": [
        {
            "image": "471137844663.dkr.ecr.us-west-2.amazonaws.com/docker-images:web_latest",
            "name": "webglimpse",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "command": [
                "python manage.py makemigrations && python manage.py migrate && gunicorn glimpseAPI.wsgi -b 0.0.0.0:8000"
            ]
        }
    ]
}