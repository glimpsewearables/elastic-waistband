{
    "family": "glimpse",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "webglimpse",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocal": "tcp"
                }
            ],
            "command": [
                "python manage.py makemigrations && python manage.py migrate && gunicorn glimpseAPI.wsgi -b 0.0.0.0:8000"
            ],
            "volumesFrom": [
                {
                    "sourceContainer": "/src",
                    "readOnly": true
                }
            ],
            "workingDirectory": "/src",
        }
    ]
}