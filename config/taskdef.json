{
    "family": "webglimpse",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:web_latest",
            "name": "webglimpse",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "command": [
                "/bin/sh", 
                "-c",
                "\"gunicorn glimpseAPI.wsgi -b 0.0.0.0:8000\""
            ]
        }
    ]
}